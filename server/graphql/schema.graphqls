# Root _Query_ definition
type Query {
  # Get _Jokes_
  jokes: Jokes
}

# _Jokes_ definition
type Jokes {
    # Get _Jokes_ by _Category_
    byCategory(category: JokeCategoryEnum): Joke
}

# _Joke_ definition
type Joke {
  id: String!
  text: String!
  categories: [JokeCategoryEnum]
}

# Available Joke's categories
enum JokeCategoryEnum {
  NERDY
  EXPLICIT
}
